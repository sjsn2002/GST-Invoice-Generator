run = "npm run dev"
entrypoint = "backend/server.js"
modules = ["nodejs-20"]

[packager]
language = "nodejs"

[packager.features]
packageSearch = true
guessImports = true

[languages.javascript]
pattern = "**/*.js"
syntax = "javascript"

[languages.jsx]
pattern = "**/*.jsx"
syntax = "jsx"

[nix]
channel = "stable-22_11"

[[ports]]
localPort = 3000
externalPort = 80
